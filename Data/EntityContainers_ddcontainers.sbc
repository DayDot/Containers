      </DefaultComponents>
    </Container>

    <Container>
      <Id>
        <TypeId>CargoContainer</TypeId>
        <SubtypeId>DDWoodChestLarge</SubtypeId>
      </Id>
      <DefaultComponents>
        <Component BuilderType="MyObjectBuilder_Inventory" SubtypeId="DDWoodChestLargeInventory"/>
        <Component BuilderType="MyObjectBuilder_InventorySpawnComponent"/>
      </DefaultComponents>
    </Container>

